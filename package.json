    "drizzle-orm": "^0.23.6",
    "focus-visible": "^5.2.0",
    "github-slugger": "^2.0.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.2.0",
    "next": "^13.2.4",
    "next-auth": "4.20.1",
