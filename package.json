{
  "name": "@codaloy/codaloy",
  "version": "0.0.1",
  "private": true,
  "main": "none",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "dbmate": "dbmate -e DATABASE_MIGRATION_URL",
    "db:migration:new": "dbmate new",
    "db:migration:up": "dbmate -e DATABASE_MIGRATION_URL migrate",
    "db:migration:down": "dbmate -e DATABASE_MIGRATION_URL rollback",
    "db:migration:status": "dbmate -e DATABASE_MIGRATION_URL status",
    "db:migration:dump": "dbmate -e DATABASE_MIGRATION_URL dump",
    "db:schema:generate": "npx zx ./scripts/introspect-database-schema.mjs",
    "clean": "rimraf .next node_modules"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@algolia/client-search": "^4.14.2",
