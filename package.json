    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "pg": "^8.12.0",
    "postcss": "^8",
    "prettier": "^3.3.2",
