    "drizzle-zod": "^0.5.1",
    "fast-glob": "^3.3.2",
    "framer-motion": "^11.2.13",
    "gray-matter": "^4.0.3",
    "lucia": "^3.2.0",
    "lucide-react": "^0.400.0",
    "nanoid": "^5.0.7",
    "next": "14.2.4",
    "next-mdx-remote": "^5.0.0",
